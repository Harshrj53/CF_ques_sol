def func(n,num):
    even=0
    odd=0
    for x in num:
        if x%2==0:
            even+=x
        else:
            odd+=x
    if even>odd:
        return "YES"
    else:
        return "NO"
    
 
t=int(input())
for _ in range(t):
    n=int(input())
    num=list(map(int,input().split()))
    print(func(n,num))
