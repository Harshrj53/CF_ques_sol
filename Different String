import random
def func(s):
    if len(s)==1 or len(set(s))==1:
        print("NO")
        return 
    while True:
        x=random.sample(s,len(s))
        if x!=list(s):
            print("YES")
            for ch in x:
                print(ch,end="")
            print()
            return 
    
            
    
t=int(input())
for _ in range(t):
    s=input()
    func(s)
